@startuml
'https://plantuml.com/class-diagram

hide circle
skinparam classAttributeIconSize 0
hide methods


class Vaccine {

    - commercialName: String
    - brand: String
    - technology used: String
}

class Dgsadmin {
    -id
    -name
    -email
}

class VaccineType {
    -code
    -disease
    -description
}

Dgsadmin "1" -- "1..*" VaccineType : configures >
VaccineType "1" -- "0..*" Vaccine : has >

@enduml