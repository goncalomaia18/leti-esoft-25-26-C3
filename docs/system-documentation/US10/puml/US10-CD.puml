@startuml
'https://plantuml.com/class-diagram

hide circle
skinparam classAttributeIconSize 0

title US10 - Specify a New Vaccine Type - Class Diagram

class NewVaccineTypeView  {
}

class NewVaccineTypeController{
    +NewVaccineTypeController()
}

class Company{
    -designation
    +getVaccineTypeContainer(): VaccineTypeContainer
}

class VaccineTypeContainer{
    +create(code, disease, description): VaccineType
    +save(VaccineType): Result
}

class VaccineType{
    -code: String
    -disease: String
    -description: String
}

NewVaccineTypeView ..> NewVaccineTypeController

NewVaccineTypeController ..> Company : company
NewVaccineTypeController ..> VaccineTypeContainer : vaccineTypeContainer

Company "1" --> "1" VaccineTypeContainer

VaccineTypeContainer "1" --> "0..*" VaccineType : vaccineTypes

@enduml